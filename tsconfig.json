{
  "compilerOptions": {
    // "target": "es5",
    // "useDefineForClassFields": true,
    // "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "target": "es2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    // "strict": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/app/*"
      ]
    }
    // "baseUrl": "src/*",
    // "paths": {
    //   "@/components/*": ["components/*"]
    // }
  },
  // "include": [
  //   "new-types.d.ts",
  //   "next-env.d.ts",
  //   ".next/types/**/*.ts",
  //   "**/*.ts",
  //   "**/*.tsx"
  // ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/components/Modal.tsx.backup",
    "src/app/components/share.jsx",
    "dist/types/**/*.ts"
, "src/app/not-found.tsx"  ],
  "exclude": [
    "node_modules",
    "src/app/components/Modal.tsx.backup"
    // "src/app/components/Modal.tsx",
    // "src/app/components/Pin.tsx"
  ]
}
